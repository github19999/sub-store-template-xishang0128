{
  "log": {
    "level": "trace",
    "timestamp": true
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "store_rdrc": true,
      "rdrc_timeout": "1h",
      "store_fakeip": true
    },
    "clash_api": {
      "default_mode": "国内直连,国外代理",
      "external_controller": "127.0.0.1:9191",
      "secret": ""
    }
  },
  "dns": {
    "servers": [
      {
        "tag": "dns_proxy_with_ecs",
        "address": "https://8.8.8.8/dns-query",
        "address_resolver": "dns_resovler",
        "detour": "proxy"
      },
      {
        "tag": "dns_local",
        "address": "https://223.5.5.5/dns-query",
        "address_resolver": "dns_resovler",
        "detour": "freedom"
      },
      {
        "tag": "dns_proxy_without_ecs",
        "address": "https://8.8.8.8/dns-query",
        "address_resolver": "dns_resovler",
        "detour": "proxy"
      },
      {
        "tag": "dns_freedom",
        "address": "https://223.5.5.5/dns-query",
        "address_resolver": "dns_resovler",
        "detour": "freedom"
      },
      {
        "tag": "dns_resovler",
        "address": "udp://223.5.5.5",
        "detour": "freedom"
      },
      {
        "tag": "dns_fakeip",
        "address": "fakeip",
        "strategy": "ipv4_only"
      }
    ],
    "strategy": "prefer_ipv4",
    "reverse_mapping": true,
    "final": "dns_proxy_without_ecs",
    "rules": [
      {
        "outbound": [
          "any"
        ],
        "server": "dns_freedom"
      },
      {
        "clash_mode": "全部直连",
        "server": "dns_local"
      },
      {
        "inbound": [
          "tun-FakeIP"
        ],
        "clash_mode": "全部代理",
        "server": "dns_fakeip"
      },
      {
        "clash_mode": "全部代理",
        "server": "dns_proxy_without_ecs"
      },
      {
        "rule_set": [
          "geosite-cn",
          "geosite-private"
        ],
        "server": "dns_local"
      },
      {
        "inbound": [
          "tun-RealIP"
        ],
        "server": "dns_proxy_without_ecs"
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "inbound": [
          "tun-FakeIP"
        ],
        "server": "dns_fakeip"
      }
    ],
    "independent_cache": true,
    "fakeip": {
      "enabled": true,
      "inet4_range": "198.18.0.0/15"
    }
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-FakeIP",
      "interface_name": "",
      "inet4_address": "172.19.0.1/30",
      "stack": "gvisor",
      "mtu": 9000,
      "sniff": true,
      "strict_route": false,
      "auto_route": true
    }
  ],
  "outbounds": [
    {
      "type": "selector",
      "tag": "proxy",
      "interrupt_exist_connections": true,
      "outbounds": [
        "自动选择"
      ],
      "default": "自动选择"
    },
    {
      "type": "selector",
      "tag": "自动选择",
      "outbounds": [
        
      ]
    },
    {
      "type": "selector",
      "tag": "Rules::CN",
      "outbounds": [
        "proxy",
        "freedom",
        "block"
      ],
      "default": "freedom"
    },
    {
      "type": "selector",
      "tag": "Rules::!CN",
      "outbounds": [
        "proxy",
        "freedom",
        "block"
      ],
      "default": "proxy"
    },
    {
      "type": "block",
      "tag": "block"
    },
    {
      "type": "direct",
      "tag": "freedom"
    },
    {
      "type": "dns",
      "tag": "dns-out"
    },
    {
      "type": "urltest",
      "tag": "autoSelete",
      "outbounds": [
        "自动选择"
      ],
      "url": "https://www.gstatic.com/generate_204",
      "interval": "5m",
      "tolerance": 70
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "rule_set": [
      {
        "type": "remote",
        "tag": "geoip-cn",
        "format": "binary",
        "url": "https://fastly.jsdelivr.net/gh/SagerNet/sing-geoip@rule-set/geoip-cn.srs",
        "download_detour": "proxy",
        "update_interval": "3d"
      },
      {
        "type": "remote",
        "tag": "geosite-cn",
        "format": "binary",
        "url": "https://fastly.jsdelivr.net/gh/SagerNet/sing-geosite@rule-set/geosite-geolocation-cn.srs",
        "download_detour": "proxy",
        "update_interval": "3d"
      },
      {
        "type": "remote",
        "tag": "geosite-private",
        "format": "binary",
        "url": "https://fastly.jsdelivr.net/gh/SagerNet/sing-geosite@rule-set/geosite-private.srs",
        "download_detour": "proxy",
        "update_interval": "3d"
      }
    ],
    "geoip": {
      "download_url": "https://github.com/soffchen/sing-geoip/releases/latest/download/geoip.db",
      "download_detour": "proxy"
    },
    "geosite": {
      "download_url": "https://github.com/soffchen/sing-geosite/releases/latest/download/geosite.db",
      "download_detour": "proxy"
    },
    "final": "Rules::!CN",
    "rules": [
      {
        "protocol": "dns",
        "outbound": "dns-out"
      },
      {
        "inbound": [
          "UIFAPI"
        ],
        "domain_suffix": [
          "baidu.com"
        ],
        "outbound": "freedom"
      },
      {
        "inbound": [
          "UIFAPI"
        ],
        "outbound": "proxy"
      },
      {
        "inbound": [
          "UIFAPIDirect"
        ],
        "outbound": "freedom"
      },
      {
        "inbound": [
          "tun-FakeIP"
        ],
        "outbound": "Rules::!CN",
        "ip_cidr": [
          "198.18.0.0/15"
        ]
      },
      {
        "rule_set": [
          "geosite-private"
        ],
        "geoip": [
          "private"
        ],
        "outbound": "freedom"
      },
      {
        "clash_mode": "全部直连",
        "outbound": "Rules::CN"
      },
      {
        "clash_mode": "全部代理",
        "outbound": "Rules::!CN"
      },
      {
        "rule_set": [
          "geosite-cn"
        ],
        "outbound": "Rules::CN"
      },
      {
        "rule_set": [
          "geoip-cn"
        ],
        "outbound": "Rules::CN"
      }
    ]
  }
}
