{
  "log": {
    "level": "debug",
    "timestamp": true
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "store_rdrc": true,
      "rdrc_timeout": "1h",
      "store_fakeip": true
    },
    "clash_api": {
      "default_mode": "国内直连,国外代理",
      "external_controller": "127.0.0.1:9191",
      "secret": ""
    }
  },
  "dns": {
    "servers": [
      {
        "tag": "dns_proxy_with_ecs",
        "address": "https://dns.google/dns-query",
        "address_resolver": "dns_resovler",
        "detour": "proxy"
      },
      {
        "tag": "dns_local",
        "address": "https://dns.alidns.com/dns-query",
        "address_resolver": "dns_resovler",
        "detour": "freedom"
      },
      {
        "tag": "dns_proxy_without_ecs",
        "address": "https://dns.google/dns-query",
        "address_resolver": "dns_resovler",
        "detour": "proxy"
      },
      {
        "tag": "dns_freedom",
        "address": "https://dns.alidns.com/dns-query",
        "address_resolver": "dns_resovler",
        "detour": "freedom"
      },
      {
        "tag": "dns_resovler",
        "address": "udp://223.5.5.5",
        "detour": "freedom"
      },
      {
        "tag": "dns_fakeip",
        "address": "fakeip",
        "strategy": "ipv4_only"
      }
    ],
    "strategy": "prefer_ipv4",
    "final": "dns_proxy_without_ecs",
    "rules": [
      {
        "outbound": [
          "any"
        ],
        "server": "dns_freedom"
      },
      {
        "clash_mode": "全部直连",
        "server": "dns_local"
      },
      {
        "inbound": [
          "tun-FakeIP"
        ],
        "clash_mode": "全部代理",
        "server": "dns_fakeip"
      },
      {
        "clash_mode": "全部代理",
        "server": "dns_proxy_without_ecs"
      },
      {
        "rule_set": [
          "geosite-private"
        ],
        "server": "dns_local"
      },
      {
        "rule_set": [
          "geoip-cn"
        ],
        "server": "dns_local"
      },
      {
        "inbound": [
          "tun-RealIP"
        ],
        "server": "dns_proxy_without_ecs"
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "inbound": [
          "tun-FakeIP"
        ],
        "server": "dns_fakeip"
      }
    ],
    "independent_cache": true,
    "fakeip": {
      "enabled": true,
      "inet4_range": "198.18.0.0/15"
    }
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-FakeIP",
      "interface_name": "",
      "address": [
        "172.19.0.1/30"
      ],
      "stack": "gvisor",
      "mtu": 9000,
      "strict_route": false,
      "auto_route": true
    }
  ],
  "outbounds": [
    {
      "type": "selector",
      "tag": "proxy",
      "interrupt_exist_connections": true,
      "outbounds": [
        "all",
        "freedom"
      ],
      "default": "all"
    },
    {
      "type": "selector",
      "tag": "all",
      "outbounds": [

      ]
    },
    {
      "type": "direct",
      "tag": "freedom"
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "rule_set": [
      {
        "type": "remote",
        "tag": "geoip-cn",
        "format": "binary",
        "url": "https://fastly.jsdelivr.net/gh/SagerNet/sing-geoip@rule-set/geoip-cn.srs",
        "download_detour": "proxy",
        "update_interval": "7d"
      },
      {
        "type": "remote",
        "tag": "geosite-cn",
        "format": "binary",
        "url": "https://fastly.jsdelivr.net/gh/SagerNet/sing-geosite@rule-set/geosite-geolocation-cn.srs",
        "download_detour": "proxy",
        "update_interval": "7d"
      },
      {
        "type": "remote",
        "tag": "geosite-private",
        "format": "binary",
        "url": "https://fastly.jsdelivr.net/gh/SagerNet/sing-geosite@rule-set/geosite-private.srs",
        "download_detour": "proxy",
        "update_interval": "7d"
      }
    ],
    "final": "proxy",
    "rules": [
      {
        "action": "sniff",
        "sniffer": [],
        "timeout": "100ms"
      },
      {
        "action": "hijack-dns",
        "protocol": "dns"
      },
      {
        "inbound": [
          "UIFAPI"
        ],
        "domain_suffix": [
          "baidu.com"
        ],
        "action": "route",
        "outbound": "freedom"
      },
      {
        "inbound": [
          "UIFAPI"
        ],
        "action": "route",
        "outbound": "proxy"
      },
      {
        "inbound": [
          "UIFAPIDirect"
        ],
        "action": "route",
        "outbound": "freedom"
      },
      {
        "inbound": [
          "tun-FakeIP"
        ],
        "outbound": "proxy",
        "ip_cidr": [
          "198.18.0.0/15"
        ]
      },
      {
        "rule_set": [
          "geosite-private"
        ],
        "ip_is_private": true,
        "outbound": "freedom"
      },
      {
        "clash_mode": "全部直连",
        "outbound": "freedom"
      },
      {
        "clash_mode": "全部代理",
        "outbound": "proxy"
      },
      {
        "rule_set": [
          "geoip-cn"
        ],
        "clash_mode": "国内直连,国外代理",
        "outbound": "freedom"
      }
    ]
  }
}
